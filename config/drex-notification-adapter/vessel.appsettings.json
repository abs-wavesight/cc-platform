{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Diagnostics.HealthChecks": "Warning"
    }
  },
  "CommonCoreLoggingSettings": {
    "InternalFileSizeLimitMB": 10,
    "LogFilesPath": "logs",
    "RetainedInternalFileCountLimit": 7,
    "OpenTelemetry": {
      "Exporters": []
    }
  },
  "ConnectionSettings": {
    "Key": "local",
    "Host": "rabbitmq-vessel",
    "Port": 5672,
    "HttpPort": 15672,
    "Protocol": "amqp",
    "vHost": "commoncore",
    "TriggerQueueName": "q.cc.drex.inbox.broadcast.adapter.drex-notification-adapter"
  },
  "SkipCertificateImport": false
}
