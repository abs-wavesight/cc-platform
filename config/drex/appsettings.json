{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Diagnostics.HealthChecks": "Warning"
    }
  },
  "ConfigFileMaxSizeInMB": 1,
  "AllowedHosts": "*",
  "LocalMessageBus": {
    "Key": "local",
    "Host": "rabbitmq-local",
    "Port": 5671,
    "HttpPort": 15671,
    "Protocol": "$RMQ_LOCAL_PROTOCOL"
  },
  "CentralMessageBus": {
    "Key": "central",
    "Host": "$RMQ_CENTRAL_HOST",
    "Port": 5671,
    "HttpPort": 15671,
    "Protocol": "$RMQ_CENTRAL_PROTOCOL",
    "vHost": "$RMQ_CENTRAL_VHOST"
  },
  "VesselImo": "$VESSEL_IMO",
  "SftpSettings": {
    "MaxRetryAttempts": 5,
    "RetryAttemptBackoffSecondsFactor": 3,
    "ConnectivityRetrySeconds": 30,
    "ConnectivityLossLogFrequencySeconds": 60
  },
  "RemoteConnectionRetryDelaySeconds": 5,
  "HealthChecks": {
    "HealthCheckProbePollFrequencySeconds": 15,
    "HealthCheckProbeTcpPort": 5001
  },
  "ConcurrencySettings": {
    "NumberOfWorkers": 2,
    "MaxParallelism": 10
  },
  "SkipCertificateImport": false
}