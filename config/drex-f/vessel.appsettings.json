{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Abs.CommonCore.Drex": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Extensions.Diagnostics.HealthChecks": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConfigFileMaxSizeInMb": 1,
  "ConnectionSettings": {
    "Host": "rabbitmq-vessel",
    "Port": 5671,
    "HttpPort": 15671,
    "Protocol": "amqps",
    "vHost": "commoncore",
    "TriggerQueueName": "q.cc.drex.adapter.mailbox.drex-f"
  },
  "VesselImo": "$VESSEL_IMO",
  "TempFolderPath": "C:/data/temp",
  "SftpSettings": {
    "Key": "central",
    "MaxRetryAttempts": 5,
    "RetryAttemptBackoffSecondsFactor": 3,
    "ConnectivityRetrySeconds": 30,
    "ConnectivityLossLogFrequencySeconds": 60,
    "host": "$SFTP_CENTRAL_HOST",
    "port": 1022,
    "protocol": "sftp"
  },
  "RemoteConnectionRetryDelaySeconds": 5,
  "HealthChecks": {
    "HealthCheckProbePollFrequencySeconds": 15,
    "HealthCheckProbeTcpPort": 5002
  },
  "ConcurrencySettings": {
    "NumberOfWorkers": 2,
    "MaxParallelism": 10
  },
  "SkipCertificateImport": false,
  "TransportMethod": "cc",
  "MaxChunkSizeInMb": 125
}